{
  "ServiceName": "autorepost-api",
  "SourceConfiguration": {
    "ImageRepository": {
      "ImageIdentifier": "379262059058.dkr.ecr.us-east-1.amazonaws.com/autorepost-api:latest",
      "ImageConfiguration": {
        "Port": "4000",
        "RuntimeEnvironmentVariables": {
          "NODE_ENV": "production",
          "PORT": "4000"
        }
      },
      "ImageRepositoryType": "ECR"
    },
    "AutoDeploymentsEnabled": false,
    "AuthenticationConfiguration": {
      "AccessRoleArn": "arn:aws:iam::379262059058:role/AppRunnerECRAccessRole"
    }
  },
  "InstanceConfiguration": {
    "Cpu": "1 vCPU",
    "Memory": "2 GB",
    "InstanceRoleArn": "arn:aws:iam::379262059058:role/AppRunnerInstanceRole"
  },
  "HealthCheckConfiguration": {
    "Protocol": "HTTP",
    "Path": "/health",
    "Interval": 10,
    "Timeout": 5,
    "HealthyThreshold": 1,
    "UnhealthyThreshold": 5
  },
  "AutoScalingConfigurationArn": "arn:aws:apprunner:us-east-1:379262059058:autoscalingconfiguration/DefaultConfiguration/1/00000000000000000000000000000001"
}
